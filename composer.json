{
    "name": "laffesz/laravel-full-calendar",
    "description": "Laravel helper for FullCalendar.io. Fork of https://github.com/maddhatter/laravel-fullcalendar",
    "keywords": [
        "laravel",
        "fullcalendar",
        "calendar"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Zolt√°n Varga",
            "email": "laffesz@gmail.com",
            "homepage": "https://laffesz.github.io",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/Laffesz/laravel-full-calendar",
    "support": {
      "email": "laffesz@gmail.com",
      "issues": "https://github.com/Laffesz/laravel-full-calendar/issues",
      "wiki": "https://github.com/Laffesz/laravel-full-calendar/wiki",
      "source": "https://github.com/Laffesz/laravel-full-calendar",
      "docs": "https://github.com/Laffesz/laravel-full-calendar"
    },
    "require": {
        "php": "^8.0|^8.2|^8.3",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/view": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "laravel/helpers": "^1.0"
    },
    "require-dev": {
      "phpstan/phpstan": "^2.1.1",
      "phpstan/phpstan-strict-rules": "^2.0.1",
      "symplify/easy-coding-standard": "^12.5.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "LaravelFullCalendar\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelFullCalendar\\FullCalendarServiceProvider"
            ],
            "aliases": {
                "Calendar": "LaravelFullCalendar\\Facades\\Calendar"
            }
        }
    },
    "scripts": {
      "cs": [
        "@php ecs check --no-interaction --no-progress-bar --ansi"
      ],
      "sa": [
        "@php vendor/bin/phpstan analyse --no-progress --ansi"
      ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
